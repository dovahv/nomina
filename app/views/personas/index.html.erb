<p id="notice"><%= notice %></p>

<h1>Listing Personas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo cedula</th>
      <th>Cedula</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Lista</th>
      <th>Telefono fijo</th>
      <th>Telefono movil</th>
      <th>Fecha de nacimiento</th>
      <th>Correo</th>
      <th>Direccion</th>
      <th>Sexo</th>
      <th>Estado civil</th>
      <th>Grado</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.tipo_cedula %></td>
        <td><%= persona.cedula %></td>
        <td><%= persona.nombres %></td>
        <td><%= persona.apellidos %></td>
        <td><%= persona.lista %></td>
        <td><%= persona.telefono_fijo %></td>
        <td><%= persona.telefono_movil %></td>
        <td><%= persona.fecha_de_nacimiento %></td>
        <td><%= persona.correo %></td>
        <td><%= persona.direccion %></td>
        <td><%= persona.sexo %></td>
        <td><%= persona.estado_civil %></td>
        <td><%= persona.grado %></td>
        <td><%= persona.status %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persona', new_persona_path %>
