<p id="notice"><%= notice %></p>

<h1>Listing Dependencias</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dependencias.each do |dependencia| %>
      <tr>
        <td><%= dependencia.nombre %></td>
        <td><%= link_to 'Show', dependencia %></td>
        <td><%= link_to 'Edit', edit_dependencia_path(dependencia) %></td>
        <td><%= link_to 'Destroy', dependencia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dependencia', new_dependencia_path %>
